{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "raviintance"
		},
		"ADLS_Destination_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_Destination'"
		},
		"GitHub1_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHub1'"
		},
		"SqlDB_sourcedata_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlDB_sourcedata'"
		},
		"ADLS_Destination_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stgadlsmissionone.dfs.core.windows.net/"
		},
		"GitHub1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "rsin134"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "raviintance_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_Destination_accountKey": {
						"value": "[parameters('ADLS_Destination_accountKey')]"
					},
					"GitHub1_credential": {
						"value": "[parameters('GitHub1_credential')]"
					},
					"SqlDB_sourcedata_connectionString": {
						"value": "[parameters('SqlDB_sourcedata_connectionString')]"
					},
					"ADLS_Destination_properties_typeProperties_url": {
						"value": "[parameters('ADLS_Destination_properties_typeProperties_url')]"
					},
					"GitHub1_properties_typeProperties_username": {
						"value": "[parameters('GitHub1_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}